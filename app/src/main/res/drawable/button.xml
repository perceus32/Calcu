<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <gradient android:startColor="#ed8c32" android:endColor="#f03232"></gradient>
            <corners android:radius="100dp"></corners>
            <stroke android:color="#4287f5" android:width="3.2sp"></stroke>


        </shape>

    </item>
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:radius="100dp"></corners>
            <solid android:color="#958de0"></solid>
            <stroke android:color="#ed8c32" android:width="3.2sp"></stroke>
        </shape>
    </item>
</selector>